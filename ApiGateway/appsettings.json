{
  "ReverseProxy": {
    "Routes": {
      "catalog_route": {
        "ClusterId": "catalog_cluster",
        "Match": {
          "Path": "/api/catalog/{**catch-all}"
        }
      },
      "basket_route": {
        "ClusterId": "basket_cluster",
        "Match": {
          "Path": "/api/basket/{**catch-all}"
        }
      },
      "order_route": {
        "ClusterId": "order_cluster",
        "Match": {
          "Path": "/api/orders/{**catch-all}"
        }
      },
      "payment_route": {
        "ClusterId": "payment_cluster",
        "Match": {
          "Path": "/api/payments/{**catch-all}"
        }
      }
    },

    "Clusters": {
      "catalog_cluster": {
        "Destinations": {
          "catalog_destination": {
            "Address": "http://localhost:5278/"
          }
        }
      },
      "basket_cluster": {
        "Destinations": {
          "basket_destination": {
            "Address": "http://localhost:5217/"
          }
        }
      },
      "order_cluster": {
        "Destinations": {
          "order_destination": {
            "Address": "http://localhost:5127/"
          }
        }
      },
      "payment_cluster": {
        "Destinations": {
          "payment_destination": {
            "Address": "http://localhost:5220/"
          }
        }
      }
    }
  }
}
